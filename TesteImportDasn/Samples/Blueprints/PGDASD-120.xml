      <Field position="5"		type="string"	  size="20"					        						          name="Pgdasd_Versao" />
      
			<Field position="6"		type="string"	  size="14"					        						          name="Cnpjmatriz" />
			<Field position="7"		type="string"	  size="150"				        							        name="Nome" />
