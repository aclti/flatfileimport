			<Field position="2"		type="decimal"	size="17"	precision="2"	persist="true"							name="VALOR" />
		</Fields>
	</Line>

	<Line name="D3002" regex="^D3002" parent="D1000">
		<Fields>
			<Field position="0"		type="string"	size="5"					persist="false"							name="REG" />
