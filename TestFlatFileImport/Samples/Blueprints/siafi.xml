<?xml version="1.0" encoding="utf-8" ?>
<Root>
	<Config>
		<!-- Character, Position-->
		<FieldSeparationType>Position</FieldSeparationType>
		<Splitter></Splitter>
		<UseResgister>false</UseResgister>
	</Config>

	<Regex>
		<Rule id="date"><![CDATA[(?<year>[12][0-9]{3})(?<month>1[0-2]|0[1-9])(?<day>0[1-9]|[1-2][0-9]|3[0-1])]]></Rule>
		<Rule id="datetime"><![CDATA[(?<year>[12][0-9]{3})(?<month>1[0-2]|0[1-9])(?<day>0[1-9]|[1-2][0-9]|3[0-1])(?<hour>[01][0-9]|2[0-3])(?<minute>[0-5][0-9])(?<second>[0-5][0-9])]]></Rule>
		<Rule id="period"><![CDATA[(?<year>[12][0-9]{3})(?<month>1[0-2]|0[1-9])]]></Rule>
		<Rule id="numdoc"><![CDATA[(?<year>[12][0-9]{3})(?<const>DR)(?<numseq>[0-9]{6})]]></Rule>
		<Rule id="rangenumber_1-4">[1-4]</Rule>
		<Rule id="rangenumber_1-6">[1-6]</Rule>
		<Rule id="operacao">[arAR]</Rule>
		<Rule id="optantess">[snSN]</Rule>
		<Rule id="limiteapur">[0-1]</Rule>
		<Rule id="money">^[0-9]+,[0-9]{2}$</Rule>
		<Rule id="esfera">[EMem]</Rule>
	</Regex>

	<Header>
		<Line class="Header" regex="^1.{72}0[1-3].{425}$" mandatory="true">
			<Fields>
				<Field position="1"		type="string"	size="1"	persit="false"					attribute="CodRegistro" />
				<Field position="2"		type="int"		size="8"	persit="true"					attribute="NumSeqRegistro" />
				<Field position="10"	type="string"	size="20"	persit="true"					attribute="CodConvenio" />
				<Field position="30"	type="datetime" size="8"	persit="true"	regex="date"	attribute="DtGeracao" />
				<Field position="38"	type="int"		size="6"	persit="true"					attribute="NumRemessa" />
				<Field position="44"	type="string"	size="2"	persit="true"					attribute="NumVersao" />
				<Field position="46"	type="string"	size="22"	persit="false"					attribute="FillerA" />
				<Field position="68"	type="string"	size="8"	persit="true"					attribute="DecendioReferencia" />
				<Field position="76"	type="string"	size="425"  persit="false"					attribute="FillerB" />
			</Fields>
		</Line>
	</Header>

	<Footer>
		<Line class="Trailer" regex="^9[0-9]{8}[0-9]{6}[0-9]{17}[A-Za-z0-9 ]{468}$" mandatory="true">
			<Fields>
				<Field position="1"		type="string"	size="1"					persit="false"	attribute="CodRegistro" />
				<Field position="2"		type="int"		size="8"					persit="true"	attribute="NumSeqRegistro" />
				<Field position="10"	type="int"		size="6"					persit="true"	attribute="TotalRegistros" />
				<Field position="16"	type="decimal"	size="17"	precision="2"	persit="true"	attribute="ValorTotalRecebido" />
				<Field position="33"	type="string"	size="468"					persit="false"	attribute="Filler" />
			</Fields>
		</Line>
	</Footer>

	<Registries>
		<Register class="" begin="" end="" />
	</Registries>

	<Line class="Details" regex="^2.{67}98.{18}98.{9}[ME].{355}98.{43}$" mandatory="true">
		<Fields>
			<Field position="1"		type="string"	size="1"					persit="false"					attribute="CodRegistro" />
			<Field position="2"		type="int"		size="8"					persit="true"					attribute="NumSeqRegistro" />
			<Field position="10"	type="datetime" size="8"					persit="true"	regex="date"	attribute="DtEmissao" />
			<Field position="18"	type="datetime" size="8"					persit="true"	regex="date"	attribute="DtVenciemnto" />
			<Field position="26"	type="string"	size="12"					persit="true"	regex="numdoc"	attribute="NumDocumento" />
			<Field position="38"	type="int"		size="6"					persit="true"					attribute="CodUnidGestEmit" />
			<Field position="44"	type="int"		size="5"					persit="true"					attribute="CodGestEmit" />
			<Field position="49"	type="int"		size="6"					persit="true"					attribute="CotUnidGestTom" />
			<Field position="55"	type="string"	size="14"					persit="true"					attribute="CnpjUnidGestTom" />
			<Field position="69"	type="string"	size="6"					persit="true"					attribute="CodMunUnidGestTom" />
			<Field position="75"	type="string"	size="14"					persit="true"					attribute="NumDocReceitaSubstituto" />
			<Field position="89"	type="string"	size="6"					persit="true"					attribute="CodMunNfse" />
			<Field position="95"	type="string"	size="5"					persit="true"					attribute="CodigoReceita" />
			<Field position="100"	type="string"	size="1"					persit="true"	regex="esfera"	attribute="EsferaReceita" />
			<Field position="101"	type="string"	size="6"					persit="true"	regex="period"	attribute="Competencia" />
			<Field position="107"	type="decimal"	size="17"	precision="2"	persit="true"					attribute="ValorPrincipal" />
			<Field position="124"	type="decimal"	size="17"	precision="2"	persit="true"					attribute="ValorMulta" />
			<Field position="141"	type="decimal"	size="17"	precision="2"	persit="true"					attribute="ValorJuros" />
			<Field position="158"	type="string"	size="10"					persit="true"					attribute="NumeroNota" />
			<Field position="168"	type="string"	size="5"					persit="true"					attribute="SerieNota" />
			<Field position="173"	type="string"	size="2"					persit="true"					attribute="SubSerieNota" />
			<Field position="175"	type="datetime"	size="8"					persit="true"	regex="date"	attribute="DtEmissaoNota" />
			<Field position="183"	type="decimal"	size="17"	precision="2"	persit="true"					attribute="ValorNota" />
			<Field position="200"	type="decimal"	size="5"	precision="3"	persit="true"					attribute="Aliquota" />
			<Field position="205"	type="decimal"	size="17"	precision="2"	persit="true"					attribute="ValorBaseCalc" />
			<Field position="222"	type="string"	size="234"					persit="true"					attribute="Observacao" />
			<Field position="456"	type="string"	size="6"					persit="true"					attribute="CodMunFavorecido" />
			<Field position="462"	type="string"	size="39"					persit="false"					attribute="Filler" />
		</Fields>
	</Line>
</Root>