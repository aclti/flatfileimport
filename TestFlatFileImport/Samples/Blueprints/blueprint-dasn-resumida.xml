			<Field position="22" type="decimal"				precision="3"	persit="true"	attribute="ALQ_IPI" />
			<Field position="23" type="decimal"				precision="2"	persit="true"	attribute="VL_IPI" />
			<Field position="24" type="decimal"				precision="3"	persit="true"	attribute="ALQ_IRPJ" />
			<Field position="25" type="decimal"				precision="2"	persit="true"	attribute="VL_IRPJ" />
			<Field position="26" type="decimal"				precision="3"	persit="true"	attribute="ALQ_ISS" />
			<Field position="27" type="decimal"				precision="2"	persit="true"	attribute="VL_ISS" />
			<Field position="28" type="decimal"				precision="3"	persit="true"	attribute="ALQ_PIS" />
			<Field position="29" type="decimal"				precision="2"	persit="true"	attribute="VL_PIS" />

			<Field position="30" type="decimal"				precision="2"	persit="true"	attribute="DIFERENÇA" />
			<Field position="31" type="string"	size="3"					persit="true"	attribute="MAIORTRIBUTO" />
		</Fields>
	</Line>

	<Line class="P3120" regex="^03120" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"	attribute="REG" />
			<Field position="1" type="decimal"				precision="3"	persit="true"	attribute="ALQ_APUR" />

			<Field position="2" type="decimal"				precision="3"	persit="true" 	attribute="ALQ_COFINS" />
