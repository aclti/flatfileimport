	<Line class="P3111" regex="^03111" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"	attribute="REG" />
			<Field position="1" type="decimal"  			precision="2" 	persit="false"	attribute="VALOR" />
			<Field position="2" type="decimal"  			precision="3" 	persit="true"	attribute="ALQ_APUR" />
			<Field position="3" type="decimal"  			precision="2" 	persit="true"	attribute="VL_IMPOSTO" />

