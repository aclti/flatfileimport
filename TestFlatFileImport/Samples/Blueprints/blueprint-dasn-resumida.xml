<?xml version="1.0" encoding="utf-8" ?>
<Root>
	<Config>
		<!-- Character, Position-->
		<FieldSeparationType>Character</FieldSeparationType>
		<Splitter>|</Splitter>
		<UseResgister>true</UseResgister>
	</Config>

	<Regex>
		<Rule id="date"><![CDATA[(?<year>[1-9][0-9]{3})(?<month>1[0-2]|0[1-9])(?<day>0[1-9]|[1-2][0-9]|3[0-1])]]></Rule>
		<Rule id="datetime"><![CDATA[(?<year>[1-9][0-9]{3})(?<month>1[0-2]|0[1-9])(?<day>0[1-9]|[1-2][0-9]|3[0-1])(?<hour>[01][0-9]|2[0-3])(?<minute>[0-5][0-9])(?<second>[0-5][0-9])]]></Rule>
		<Rule id="period"><![CDATA[(?<year>[1-9][0-9]{3})(?<month>1[0-2]|0[1-9])]]></Rule>
		<Rule id="money">^[0-9]+,[0-9]{2}$</Rule>
		<Rule id="decimal">^[0-9]+,[0-9]+$</Rule>
		<Rule id="int">^[0-9]+$</Rule>
		<Rule id="rangenumber_1-4">[1-4]</Rule>		
		<Rule id="rangenumber_1-6">[1-6]</Rule>
		<Rule id="operacao">[arAR]</Rule>
		<Rule id="optantess">[snSN]</Rule>
		<Rule id="limiteapur">[0-1]</Rule>		
	</Regex>

	<!-- LineType: Header, Footer, Datails (default) -->
	<!-- Occurrence NoOrMany, One, AtLeastOne, Gap[MIN-MAX] -->
	<Line class="AAAAA" regex="^AAAAA" mandatory="true" occurrence="One" type="Header" begin="" end="ZZZZZ" parent="">
		<Fields>
			<Field position="0" type="string"	size="5" persit="false"					attribute="tipo_registro" />
			<Field position="1" type="string"	size="3" persit="true"					attribute="COD_VER" />
			<Field position="2" type="date"		size="8" persit="true"	regex="date"	attribute="DT_INICIO" />
			<Field position="3" type="date"		size="8" persit="true"	regex="date"	attribute="DT_FIM" />
		</Fields>
	</Line>
	
	<Line class="ZZZZZ" regex="^ZZZZZ" mandatory="true" type="Footer" begin="AAAAA" end="" parent="">
		<Fields>
			<Field position="0"	type="string" size="5"	persit="false"				attribute="tipo_registro" />
			<Field position="1" type="int"				persit="false" regex="int"	attribute="qtd_lin_reg_zzzzz" />
		</Fields>
	</Line>

	<Line class="D1000" regex="^D1000" mandatory="true" type="Header" begin="" end="D9999" parent="">
		<Fields>
			<Field position="0"		type="string"	size="5"	persit="false"						attribute="tipo_registro" />
			<Field position="1"		type="string"	size="15"	persit="true"						attribute="ID_DECLARACAO" />
			<Field position="2"		type="int"					persit="true"	regex="int"			attribute="TDSN_ID_TIPO" />
			<Field position="3"		type="int"					persit="true"	regex="int"			attribute="EXERCICIO" />
			<Field position="4"		type="string"	size="150"	persit="true"						attribute="RAZAO_SOCIAL" />
			<Field position="5"		type="date"		size="8"	persit="true"	regex="date"		attribute="DT_ABERTURA" />
			<Field position="6"		type="date"		size="8"	persit="true"	regex="date"		attribute="DT_INICIO_ATIVIDADE" />
			<Field position="7"		type="string"	size="17"	persit="true"						attribute="NUM_RECIBO" />
			<Field position="8"		type="string"	size="20"	persit="true"						attribute="NUM_AUTENTICACAO" />
			<Field position="9"		type="date"		size="14"	persit="true"	regex="datetime"	attribute="DT_TRANSMISSAO" />
			<Field position="10"	type="string"	size="9"	persit="true"						attribute="VERSAO" />
			<Field position="11"	type="string"	size="1"	persit="true"						attribute="REGIME_COMPETENCIA" />
		</Fields>
	</Line>

	<Line class="D9999" regex="^D9999" mandatory="true" type="Footer" begin="" end="" parent="">
		<Fields>
			<Field position="0" type="string" size="5" attribute="tipo_registro" />
			<Field position="1" type="int" attribute="qtd_lin_reg_d9999" />
		</Fields>
	</Line>

	<Line class="D4000" regex="^D4000" mandatory="false" occurrence="*" type="Datails" begin="" end="" parent="D1000">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"					attribute="TIPO_REGISTRO" />
			<Field position="1" type="string"	size="17"					persit="true"					attribute="NUM_SEQ" />
			<Field position="2" type="date"		size="6"					persit="true" regex="period"	attribute="PERIODO" />
			<Field position="3" type="decimal"	size="17"	precision="2"	persit="true"					attribute="VL_RECEITA_APURADA" />
			<Field position="4" type="decimal"	size="17"	precision="2"	persit="true"					attribute="VL_DEBITO_APURADO" />
			<Field position="5" type="decimal"	size="17"	precision="2"	persit="true"					attribute="VL_TOTAL_DAS_PAGOS" />
		</Fields>
	</Line>
	
	<Line class="D2000" regex="^D2000" mandatory="false">
		<Fields>
			<Field position="0"		type="string"	size="5"	persit="false"							attribute="tipo_registro" />
			<Field position="1"		type="int"					persit="true"	regex="rangenumber_1-6" attribute="ID_EVENTO" />
			<Field position="2"		type="date"		size="8"	persit="true"	regex="date"			attribute="DT_EVENTO" />
		</Fields>
	</Line>

	<Line class="D2001" regex="^D2001" mandatory="false">
		<Fields>
			<Field position="0"		type="string"	size="5"	persit="false"							attribute="tipo_registro" />
			<Field position="1"		type="int"					persit="true"	regex="rangenumber_1-4" attribute="ID_TIPO_ESFERA" />
			<Field position="2"		type="string"	size="2"	persit="true"							attribute="UF_REPARTICAO" />
			<Field position="3"		type="string"	size="5"	persit="true"							attribute="COD_MUNIC_REPARTICAO" />
			<Field position="4"		type="string"	size="50"	persit="true"							attribute="NOME_MUNIC_REPARTICAO" />
			<Field position="5"		type="string"	size="24"	persit="true"							attribute="NUM_PROCESSO" />
			<Field position="6"		type="date"		size="6"	persit="true"	regex="period"			attribute="DT_INICIO" />
			<Field position="7"		type="date"		size="6"	persit="true"	regex="period"			attribute="DT_FIM" />
		</Fields>
	</Line>

	<Line class="D3001" regex="^D3001" mandatory="false">
		<Fields>
			<Field position="0"		type="string"	size="5"					persit="false"					attribute="TIPO_REGISTRO" />
			<Field position="1"		type="date"		size="6"					persit="true"	regex="period"	attribute="PERIODO_APURACAO" />
			<Field position="2"		type="decimal"	size="17"	precision="2"	persit="true"	regex="money"	attribute="VALOR" />
		</Fields>
	</Line>

	<Line class="D3002" regex="^D3002" mandatory="false">
		<Fields>
			<Field position="0"		type="string"	size="5"					persit="false"					attribute="TIPO_REGISTRO" />
			<Field position="1"		type="date"		size="6"					persit="true"	regex="period"	attribute="PERIODO_APURACAO" />
			<Field position="2"		type="decimal"	size="17"	precision="2"	persit="true"					attribute="VALOR" />
		</Fields>
	</Line>

	<Line class="D4000" regex="^D4000" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"					attribute="TIPO_REGISTRO" />
			<Field position="1" type="string"	size="17"					persit="true"					attribute="NUM_SEQ" />
			<Field position="2" type="date"		size="6"					persit="true" regex="period"	attribute="PERIODO" />
			<Field position="3" type="decimal"	size="17"	precision="2"	persit="true"					attribute="VL_RECEITA_APURADA" />
			<Field position="4" type="decimal"	size="17"	precision="2"	persit="true"					attribute="VL_DEBITO_APURADO" />
			<Field position="5" type="decimal"	size="17"	precision="2"	persit="true"					attribute="VL_TOTAL_DAS_PAGOS" />
		</Fields>
	</Line>

	<!-- REGISTORS PGDAS INICIO -->
	<Line class="P0000" regex="^00000" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"						attribute="REG" />
			<Field position="1" type="string"	size="14"					persit="true"						attribute="CNPJ_MATRIZ" />
			<Field position="2" type="string"	size="56"					persit="true"						attribute="NOME" />
			<Field position="3" type="string"	size="5"					persit="true"						attribute="COD_TOM" />
			<Field position="4" type="string"	size="1"					persit="true"	regex="optantess"	attribute="OPTANTE" />
			<Field position="5" type="date"		size="8"					persit="true"	regex="date"		attribute="ABERTURA" />
			<Field position="6" type="date"		size="6"					persit="true"	regex="period"		attribute="PERIODO" />
			<Field position="7" type="decimal"				precision="2"	persit="true"						attribute="RECEITA_BRUTA_PA" />
			<Field position="8" type="decimal"				precision="3"	persit="true"						attribute="RAZAO_FOLHA" />
			<Field position="9" type="decimal"				precision="2"	persit="true"						attribute="INDICE_MAJOR" />
			<Field position="10" type="string"	size="1"					persit="true"	regex="operacao"	attribute="OPERACAO" />
			<Field position="11" type="int"									persit="true"						attribute="REGIME" />
			<Field position="12" type="decimal"				precision="2"	persit="true"						attribute="RECEITA_BRUTA_PA_CAIXA" />
		</Fields>
	</Line>

	<Line class="P1000" regex="^01000" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"				attribute="REG" />
			<Field position="1" type="string"	size="17"					persit="true"				attribute="NUM_PAGTO" />
			<Field position="2" type="decimal"				precision="2"	persit="true"				attribute="VL_PRINC" />
			<Field position="3" type="decimal"				precision="2"	persit="true"				attribute="VL_MULTA" />
			<Field position="4" type="decimal"				precision="2"	persit="true"				attribute="VL_JUROS" />
			<Field position="5" type="decimal"				precision="2"	persit="true"				attribute="TOTAL_DEVIDO" />
			<Field position="6" type="date"		size="8"					persit="true" regex="date"	attribute="DT_VENCIMENTO" />
			<Field position="7" type="date"		size="8"					persit="true" regex="date"	attribute="DT_VALID_CALC" />
			<Field position="8" type="decimal"				precision="2"	persit="true"				attribute="VL_DAS" />
		</Fields>
	</Line>

	<Line class="P2000" regex="^02000" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"	attribute="REG" />
			<Field position="1" type="decimal"				precision="2"	persit="true"	attribute="RECEITA_BRUTA_12" />
			<Field position="2" type="decimal"				precision="2"	persit="true"	attribute="RECEITA_BRUTA_ANO_ANTERIOR" />
			<Field position="3" type="decimal"				precision="2"	persit="true"	attribute="RECEITA_BRUTA_ANO" />
			<Field position="4" type="decimal"				precision="2"	persit="true"	attribute="RECEITA_BRUTA_12_ORIGINAL" />
			<Field position="5" type="decimal"				precision="2"	persit="true"	attribute="RECEITA_BRUTA_12_ORIGINAL_ANTERIOR" />
			<Field position="6" type="decimal"				precision="2"	persit="true"	attribute="ICMS" />
			<Field position="7" type="decimal"				precision="2"	persit="true"	attribute="ISS" />
		</Fields>
	</Line>

	<Line class="P3000" regex="^03000" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"						attribute="REG" />
			<Field position="1" type="string"	size="14"					persit="true"						attribute="CNPJ" />
			<Field position="2" type="string"	size="2"					persit="true"						attribute="UF" />
			<Field position="3" type="string"	size="5"					persit="true"						attribute="COD_TOM" />
			<Field position="4" type="decimal"				precision="2"	persit="true"						attribute="VL_TOTAL" />
			<Field position="5" type="decimal"				precision="2"	persit="true"						attribute="IME" />
			<Field position="6" type="decimal"				precision="2"	persit="true"						attribute="LIMITE" />
			<Field position="7" type="int"									persit="true"	regex="limiteapur"	attribute="LIM_ULTRAPASSADO_PERIODO_APURACAO" />
			<Field position="8" type="decimal"	size="16"	precision="15"	persit="true"						attribute="PREX_1" />
			<Field position="9" type="decimal"	size="16"	precision="15"	persit="true"						attribute="PREX_2" />
		</Fields>
	</Line>

	<Line class="P4000" regex="^04000" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"	attribute="REG" />
			<Field position="1" type="int"									persit="true"	attribute="COD_REC_PRINC" />
			<Field position="2" type="decimal"				precision="2"	persit="true"	attribute="VALOR_PRINCIPAL" />
			<Field position="3" type="int"									persit="true"	attribute="COD_REC_MULTA" />
			<Field position="4" type="decimal"				precision="2"	persit="true"	attribute="VALOR_MULTA" />
			<Field position="5" type="int"									persit="true"	attribute="COD_REC_JUROS" />
			<Field position="6" type="decimal"				precision="2"	persit="true"	attribute="VALOR_JUROS" />
			<Field position="7" type="string"	size="2"					persit="true"	attribute="UF" />
			<Field position="8" type="string"	size="4"					persit="true"	attribute="COD_MUNIC" />
		</Fields>
	</Line>

	<Line class="P3100" regex="^03100" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"	attribute="REG" />
			<Field position="1" type="string"	size="2"					persit="true"	attribute="TIPO_ATIVIDADE" />
			<Field position="2" type="decimal"				precision="2"	persit="true"	attribute="VL_TOTAL" />
		</Fields>
	</Line>

	<Line class="P3110" regex="^03110" mandatory="false">
		<Fields>
			<Field position="0" type="string"	size="5"					persit="false"	attribute="REG" />
			<Field position="1" type="string"	size="2"					persit="true"	attribute="UF" />
			<Field position="2" type="string"	size="5"					persit="true"	attribute="COD_TOM" />
			<Field position="3" type="decimal"				precision="2"	persit="true"	attribute="VALOR" />

			<Field position="4" type="string"	size="1"					persit="true"	attribute="COFINS" />
