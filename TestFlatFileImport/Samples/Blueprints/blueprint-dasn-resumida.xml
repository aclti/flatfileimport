			<Field position="14" type="decimal"				precision="3"	persit="true"	attribute="ALQ_COFINS" />
			<Field position="15" type="decimal"				precision="2"	persit="true"	attribute="VL_COFINS" />
			<Field position="16" type="decimal"				precision="3"	persit="true"	attribute="ALQ_CSLL" />
			<Field position="17" type="decimal"				precision="2"	persit="true"	attribute="VL_CSLL" />
			<Field position="18" type="decimal"				precision="3"	persit="true"	attribute="ALQ_ICMS" />
			<Field position="19" type="decimal"				precision="2"	persit="true"	attribute="VL_ICMS" />
			<Field position="20" type="decimal"				precision="3"	persit="true"	attribute="ALQ_INSS" />
