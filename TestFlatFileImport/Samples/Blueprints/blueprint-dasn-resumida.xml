			<Field position="6" type="decimal"				precision="3"	persit="true" 	attribute="ALQ_ICMS" />
			<Field position="7" type="decimal"				precision="2"	persit="true" 	attribute="VL_ICMS" />
			<Field position="8" type="decimal"				precision="3" 	persit="true" 	attribute="ALQ_INSS" />
