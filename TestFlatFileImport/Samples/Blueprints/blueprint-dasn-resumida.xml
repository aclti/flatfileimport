			<Field position="10" type="string"	size="1"					persit="true"	attribute="ISS" />
			<Field position="11" type="string"	size="1"					persit="true"	attribute="PIS" />

			<Field position="12" type="decimal"				precision="3"	persit="true"	attribute="ALQ_APUR" />
